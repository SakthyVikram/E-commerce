{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/structure": "off",
    "axe/forms": "off",
    "axe/parsing": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]",
          "iframe[referrerpolicy=no-referrer-when-downgrade]"
        ]
      }
    ],
    "no-inline-styles": "off",
    "button-type": "off",
    "axe/text-alternatives": "off"
  }
}